<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kumoh.iat">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_iat"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_iat_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".CheckingActivity"></activity>
        <activity android:name=".__TestActivity" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AgreementActivity"
            android:label="AgreementActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LoginActivity"
            android:label="LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SurveyActivity"
            android:label="SurveyActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".SurveyTempActivity"
            android:label="SurveyTempActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".AlertActivity"
            android:label="AlertActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".TestingActivity"
            android:label="TestingActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".FinishActivity"
            android:label="FinishActivity"
            android:screenOrientation="landscape" />
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

</manifest>